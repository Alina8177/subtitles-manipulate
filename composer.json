{
    "name": "pbmedia/laravel-ffmpeg",
    "description": "FFMpeg for Laravel",
    "keywords": [
        "pbmedia",
        "laravel-ffmpeg"
    ],
    "homepage": "https://github.com/pbmedia/laravel-ffmpeg",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Pascal Baljet",
            "email": "pascal@protone.media",
            "homepage": "https://protone.media",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "illuminate/bus": "^6.0|^7.0",
        "illuminate/config": "^6.0|^7.0",
        "illuminate/filesystem": "^6.0|^7.0",
        "illuminate/log": "^6.0|^7.0",
        "illuminate/support": "^6.0|^7.0",
        "php-ffmpeg/php-ffmpeg": "^0.16.0",
        "spatie/temporary-directory": "^1.2"
    },
    "require-dev": {
        "league/flysystem-memory": "^1.0",
        "mockery/mockery": "^1.3",
        "orchestra/testbench": "^4.0|^5.0",
        "phpunit/phpunit": "^8.0",
        "twistor/flysystem-http": "^0.2.0"
    },
    "autoload": {
        "psr-4": {
            "Pbmedia\\LaravelFFMpeg\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pbmedia\\LaravelFFMpeg\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pbmedia\\LaravelFFMpeg\\Support\\FFMpegServiceProvider"
            ],
            "aliases": {
                "FFMpeg": "Pbmedia\\LaravelFFMpeg\\Support\\FFMpegFacade"
            }
        }
    }
}
